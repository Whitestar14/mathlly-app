{
  "name": "mathlly",
  "version": "0.12.2",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --open && node scripts/generate-version-info.cjs",
    "build": "vue-tsc && vite build",
    "prebuild": "node scripts/generate-version-info.cjs",
    "preview": "vite preview",
    "serve:prod": "pnpm dlx serve -s dist",
    "build:analyze": "vite build --mode production && open dist/stats.html",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "type-check": "vue-tsc --noEmit",
    "version:bump": "node scripts/bump-version.js",
    "pwa:dev": "vite --mode development",
    "pwa:build": "vite build --mode production"
  },
  "dependencies": {
    "@vueuse/core": "^11.3.0",
    "@vueuse/motion": "^3.0.3",
    "animejs": "^3.2.2",
    "chart.js": "^4.4.9",
    "dexie": "^4.0.10",
    "html-to-image": "^1.11.13",
    "lucide-vue-next": "^0.469.0",
    "mathjs": "^14.2.1",
    "pinia": "^2.3.0",
    "radix-vue": "^1.9.12",
    "tippy.js": "^6.3.7",
    "vue": "^3.5.13",
    "vue-chartjs": "^5.3.2",
    "vue-router": "^4.5.0",
    "vue-tippy": "^6.6.0",
    "workbox-window": "^7.3.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-plugin-vue": "^9.32.0",
    "postcss": "^8.4.49",
    "rollup-plugin-visualizer": "^5.12.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-pwa": "^0.20.0",
    "vue-eslint-parser": "^10.1.4",
    "vue-tsc": "^2.0.0"
  },
  "packageManager": "pnpm@9.12.3",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.0.0"
  }
}
